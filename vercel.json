{
  "cleanUrls": true,
  "trailingSlash": false,
  "public": false,
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/post/(.*)",
      "destination": "/dist/post/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/dist/$1.html"
    },
    {
      "source": "/",
      "destination": "/dist/index.html"
    }
  ]
}
